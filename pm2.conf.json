{
  "apps": {
    "name": "gomaxai-v8.5.4",
    "script": "./app/main.js",
    "watch": false,
    "ignore_watch": [
      "node_modules",
      "logs",
      "uploaded"
    ],
    "max_restarts": 10,
    "env": {
      "TZ": "Asia/Shanghai"
    },
    "instances": 1,
    "error_file": "logs/err.log",
    "out_file": "logs/out.log",
    "log_date_format": "YYYY-MM-DD HH:mm:ss",
    "max_memory_restart": "700M",
    "env_production": {
      "NODE_ENV": "production"
    }
  }
}
